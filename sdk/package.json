{
  "name": "@rainsonet/sdk",
  "version": "0.1.0",
  "description": "RAINSONET/RELYO TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@noble/hashes": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "rainsonet",
    "relyo",
    "payment",
    "decentralized",
    "cryptocurrency"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rainsonet/sdk"
  },
  "license": "MIT"
}
